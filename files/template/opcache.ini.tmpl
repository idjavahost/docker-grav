opcache.enable={{ default .Env.OPCACHE_ENABLE "On" }}
opcache.enable_cli={{ default .Env.OPCACHE_ENABLE_CLI "Off" }}
opcache.memory_consumption={{ default .Env.OPCACHE_MEMORY "128" }}
opcache.interned_strings_buffer=8
opcache.max_accelerated_files=4000
opcache.revalidate_freq=60
opcache.fast_shutdown=1
